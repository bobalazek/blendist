{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "shared-node-browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-unused-vars": "off",
    "no-console": "off",
    "no-trailing-spaces": ["error", { "skipBlankLines": false }]
  }
}
